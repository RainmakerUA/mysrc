/etc/init.d/dispatcher=20
=20
=20
#!/bin/bash=20
#=20
# chkconfig: 2345 99 10=20
# description: Starts and stops Dispatcher processes=20
#=20
# Set DISPATCHER=5FOWNER to the user id of the owner of the=20
# Dispatcher installation. If controlling multiple Dispatcher=20
# installations then separate the Dispatcher owners with white space=20
#=20
=20
DISPATCHER=5FOWNER=3D"accdba" #Separate multiple owners with white space=20
case "$1" in=20
'start')=20
# Start the Dispatcher instances=20
for DCS=5FOWNER in `echo $DISPATCHER=5FOWNER`=20
do=20
      su - $DCS=5FOWNER -c "appstart -y"=20
      su - $DCS=5FOWNER -c "startcomms -y"=20
      su - $DCS=5FOWNER -c "partitionctl -u"=20
      su - $DCS=5FOWNER -c "apachectl -u"=20
Echo =E2=80=9Cnothing to do=E2=80=9D=20
done=20
;;=20
=20
'stop')=20
# Stop the Dispatcher instances=20
for DCS=5FOWNER in `echo $DISPATCHER=5FOWNER`=20
do=20
      su - $DCS=5FOWNER -c "partitionctl -d"=20
      su - $DCS=5FOWNER -c "stopcomms -y"=20
      su - $DCS=5FOWNER -c "appstop -y"=20
      su - $DCS=5FOWNER -c "apachectl =E2=80=93d"=20
done=20
;;=20
esac # End of Script =20
# The lines =20
# chkconfig: 2345 99 10=20
# and =20
# description: Starts and stops Dispatcher processes =20
# are mandatory since they describe the characteristics =20
# of the service, where =20
# 2345 means that the service will be started in the init level 2,3,4=20
# and  5 # and will be stopped in the other levels =20
# 99 means that the service will be started at the end =20
# of the init level =20
# 10 means that the service will be stopped at the beginning =20
# of the init level  

